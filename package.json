{
  "name": "slasshy-secure-cli",
  "version": "2.0.6",
  "description": "Military-grade secure storage with steganography & Google Drive sync",
  "main": "dist/index.js",
  "bin": {
    "slasshy": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "slasshy": "tsx src/index.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlasshyOverhere/slasshy-secure-cli.git"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "security",
    "encryption",
    "steganography",
    "password-manager",
    "vault",
    "aes-256",
    "google-drive"
  ],
  "author": "Slasshy",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "argon2": "^0.31.2",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "crypto-js": "^4.2.0",
    "googleapis": "^128.0.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "pngjs": "^7.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/crypto-js": "^4.2.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "@types/pngjs": "^6.0.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
